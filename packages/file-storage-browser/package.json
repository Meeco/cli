{
  "name": "@meeco/file-storage-browser",
  "version": "5.0.0",
  "description": "Browser module for uploading and downloading encrypted files",
  "source": "src/index.ts",
  "types": "./lib/index.d.ts",
  "browser": "./lib/esm/index.js",
  "main": "./lib/umd/index.js",
  "module": "./lib/esm/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "build": "npm run build:types && npm run build:esm && npm run build:umd",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "tsc -p tsconfig.umd.json",
    "demo": "webpack-dev-server demo",
    "build:demo": "webpack",
    "format:check": "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" *.json README.md",
    "docs": "typedoc --out docs --tsconfig tsconfig.json --disableOutputCheck --excludePrivate --excludeProtected src/index.ts"
  },
  "dependencies": {
    "@meeco/cryppo": "2.0.2",
    "@meeco/file-storage-common": "^5.0.0",
    "@meeco/style-kit": "^2.0.1",
    "@meeco/vault-api-sdk": "32.0.0-stage.20210527103605.f1f76f0",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@meeco/sdk": "^3.0.1",
    "bili": "^5.0.5",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "^3.11.2"
  },
